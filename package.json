{
  "name": "optikeyboardlayout",
  "version": "0.1.0",
  "main": "dist",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "./node_modules/.bin/webpack --config ./webpack.config.prod.ts",
    "dev": "cross-env ./node_modules/.bin/webpack-dev-server --config ./webpack.config.ts --port 4201 --content-base webpackDist",
    "test": "./node_modules/.bin/jest --ci --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "update": "ncu -u --packageFile package.json",
    "prepare": "npm run build"
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.19",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "coveralls": "^3.0.9",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "react-test-renderer": "^16.12.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.2",
    "ts-jest": "=24.2.0",
    "ts-node": "^8.5.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.43",
    "@xpfw/data": "1.0.0-beta.16",
    "@xpfw/form": "^1.0.0-beta.16",
    "@xpfw/form-web": "^1.0.0-beta.16",
    "fast-json-patch": "^2.2.1",
    "lodash": "^4.17.15",
    "mobx": "^5.15.1",
    "mobx-react-lite": "^1.5.1",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-dropzone": "^10.2.1",
    "react-measure": "^2.3.0",
    "xml-js": "^1.6.11"
  }
}
