{
  "name": "optikeyboardlayout",
  "version": "0.1.0",
  "main": "dist",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "build": "./node_modules/.bin/webpack --config ./webpack.config.prod.ts",
    "dev": "cross-env ./node_modules/.bin/webpack-dev-server --config ./webpack.config.ts --port 4201 --content-base webpackDist",
    "test": "./node_modules/.bin/jest --ci",
    "update": "ncu -u --packageFile package.json",
    "prepare": "npm run build"
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.124",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webpack": "^4.4.31",
    "@types/webpack-dev-server": "^3.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "react-test-renderer": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "=24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@xpfw/data": "1.0.0-beta.16",
    "@xpfw/form": "^1.0.0-beta.16",
    "@xpfw/form-web": "^1.0.0-beta.16",
    "fast-json-patch": "^2.1.0",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react-lite": "^1.3.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^3.3.0",
    "react-dropzone": "^10.1.4",
    "xml-js": "^1.6.11"
  }
}
